{{!-- script for weekly sales data --}}
<script>
  const date=[]
  {{#each weeklyGraph}}
  date.unshift('{{this.ddmmyy}}')
  {{/each}}
  console.log(date,"unshifted date")

  const revenue=[]
  {{#each weeklyGraph}}
  revenue.unshift('{{this.revenue}}')
  {{/each}}
  console.log(revenue,"unshifted revenue")

  var xValues = date;
  var yValues = revenue;

new Chart("salesChartWeekly", {
  type: "line",
  data: {
    labels: xValues,
    datasets: [{
      fill: false,
      lineTension: 0,
      backgroundColor: "rgba(0,0,255,1.0)",
      borderColor: "rgba(0,0,255,0.1)",
      data: yValues
    }]
  },
  options: {
    legend: {display: false},
    title: {
      display: true,
      text: "Weekly Sales Report"
    }

    
  }
});

</script>


{{!-- script for monthly sales data --}}
<script>
  const monthlyChart=document.getElementById('salesChartMonthly')

const month=[]
{{#each monthlyGraph}}
month.unshift('{{this.month}}')
{{/each}}
console.log(month,"unshifted month")

const revenue=[]
{{#each monthlyGraph}}
revenue.unshift('{{this.revenue}}')
{{/each}}
console.log(revenue,"unshifted revenue")

new Chart(monthlyChart, {
    type: 'bar',
    data: {
      labels: month,
      datasets: [{
        label: 'Monthly Sales data',
        backgroundColor: "rgba(0,0,255,1.0)",
        borderColor: "rgba(0,0,255,0.1)",
        data: revenue,
        borderWidth: 1
      }]
    },
    options: {
      scales: {
        y: {
          beginAtZero: true
        }
      }
    }
  });


</script>

  

  
 
  


  